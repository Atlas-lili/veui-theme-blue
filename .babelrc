{
    "presets": [
      ["env", { "modules": false }],
      "stage-2"
    ],
    "plugins": [
      "veui",
      "transform-vue-jsx",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": false
        }
      ]
    ],
    "comments": false,
    "env": {
      "test": {
        "presets": ["env", "stage-2"],
        "plugins": ["istanbul"]
      },
      "production": {
        "plugins": ["lodash"]
      },
      "development": {
        "plugins": ["lodash"]
      }
    }
  }
  